<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appetite">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".MenuList"
            android:label="@string/title_activity_menulist" />
        <activity
            android:name=".DishDetailsActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ChefDetailsActivity"
            android:label="@string/title_activity_chef_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OrderDetailsActivity"
            android:label="@string/title_activity_order_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ReviiewList"></activity>
        <activity android:name=".AddReviewActivity"></activity>
        <activity
            android:name=".Menu"
            android:label="@string/title_activity_menu"></activity>
        <activity
            android:name=".ChefMenuItemActivity"
            android:label="@string/title_activity_chef_menu_item"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
